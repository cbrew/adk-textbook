# Environment configuration for PostgreSQL Chat Agent
# Copy this file to .env and fill in your actual values

# AI Model API Keys (choose one or more)
GOOGLE_API_KEY=your_google_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# PostgreSQL Database Configuration
# These default values work with the Docker Compose setup
DB_HOST=localhost
DB_PORT=5432
DB_NAME=adk_runtime
DB_USER=adk_user
DB_PASSWORD=adk_password

# Google Cloud Configuration (if using Google services)
GOOGLE_CLOUD_PROJECT=your_project_id
GOOGLE_CLOUD_LOCATION=us-central1

# Agent Configuration
AGENT_APP_NAME=postgres_chat_agent
AGENT_USER_ID=demo_user

# Optional: Logging Level
LOG_LEVEL=INFO